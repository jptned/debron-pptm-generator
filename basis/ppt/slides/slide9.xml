                        <a:r>
                            <a:rPr lang="nl-NL" sz="4000" dirty="0">
                                <a:solidFill>
                                    <a:srgbClr val="FFFFFF"/>
                                </a:solidFill>
                                <a:effectLst>
                                    <a:prstShdw prst="shdw14" dist="35921" dir="2700001">
